<h1>Listing wowolinks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wowolinks.each do |wowolink| %>
      <tr>
        <td><%= link_to 'Show', wowolink %></td>
        <td><%= link_to 'Edit', edit_wowolink_path(wowolink) %></td>
        <td><%= link_to 'Destroy', wowolink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wowolink', new_wowolink_path %>
